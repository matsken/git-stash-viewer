{
    "name": "git-stash-viewer",
    "displayName": "git-stash-viewer",
    "description": "Viewer for Git Stash",
    "version": "0.1.0",
    "publisher": "matsken",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:gitStash"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "views": {
            "explorer": [
                {
                    "id": "gitStash",
                    "name": "Git Stash"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}